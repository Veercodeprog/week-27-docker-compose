
generator client {
  provider = "prisma-client-js"
  // output = "../../node_modules/.prisma/client"
  // output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
    id        String          @id        @default(uuid())
    username  String
    password  String
}
